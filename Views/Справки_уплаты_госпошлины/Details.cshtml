@model WebKursovaya.Models.Справки_уплаты_госпошлины

@{
    ViewData["Title"] = "Подробности записи";
}

<h1>Подробности</h1>

<div>
    <h4>записи о справке уплаты госпошлины</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Заявитель)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ЗаявительФИО)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Дата)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Дата.Дата)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Номер_справки_уплаты_госпошлины" class="btn btn-primary">Редактировать</a> |
    <a asp-action="Index" class="btn btn-dark">Вернуться к списку</a>
</div>
